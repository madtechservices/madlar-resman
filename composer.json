{
    "name": "tomatophp/tomato-ketchup",
    "type": "library",
    "description": "a resource manager class for tomato php",
    "keywords": [
        "php",
        "laravel",
        "resource",
        "CRUD",
        "Nova"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoKetchup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoKetchup\\TomatoKetchupServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0",
        "tomatophp/tomato-components": "^1.0",
        "tomatophp/tomato-admin": "^1.0"
    }
}
